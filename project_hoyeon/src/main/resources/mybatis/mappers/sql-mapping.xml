<mapper namespace="users.UsersDAO" >

<select id="getUsersList" resultType="users.UsersVO">
 select * from member
</select>

<select id="getStudyinfoList" resultType="users.UsersVO">
 select * from STUDYINFO
</select>

<delete id="deleteBoard" parameterType="users.UsersVO">
delete from STUDYINFO where STUDY_ID like #{study_id}
</delete>

<select id="getTitle" resultType="String">
select title from STUDYINFO <!-- where member_id = #{member_id} -->
</select>

<select id="getWord" resultType="users.UsersVO" parameterType="String">
select title from STUDYINFO where title = #{word} 
</select>


